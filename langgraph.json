{
  "graphs": {
    "supervisor": "mcp_client/test_react.py:setup_graph",
    "agent-supervisor": "mcp_client/agent_getinsights.py:build_agent"
  },
  "dependencies": [
    "git+https://github.com/langchain-ai/langchain-mcp-adapters.git#egg=langchain_mcp_adapters",
    "langchain-openai>=0.1.8",
    "langgraph>=0.0.51",
    "fastmcp>=2.2.5",
    "langchain>=0.1.20",
    "openai>=1.23.0",
    "oci==2.141.1",
    "langchain_core==0.3.29",
    "langchain-community==0.3.14",
    "nemoguardrails"
  ],
  "mounts": [
    {
      "source": "~/.oci/config",
      "target": "/home/app/.oci/config"
    },
    {
      "source": "~/.oci/oci_api_key.pem",
      "target": "/home/app/.oci/oci_api_key.pem"
    }
  ]
}
